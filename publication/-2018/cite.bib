@article{__2018,
 abstract = {人工林是城市具生命绿色基础设施的重要组成部分,土壤是人工林健康可持续发展的基础。在上海市域范围内选取5种主要人工林群落类型样地34个,分层采集0~100 cm土壤剖面样品,对人工林土壤理化性质与植物群落特征的相关性进行了研究。结果表明:(1)上海市人工林土壤容重偏高(1.07~1.67 g·cm~(-3)),土壤pH 5.7~8.3,土壤电导率(EC)为0.03~0.40 mS·cm~(-1),碱解氮(5.4~183.4 mg·kg~(-1)),有效磷(1.4~80.5 mg·kg~(-1))和有机质(3.33~42.88 g·kg~(-1))相对匮乏;土壤pH值、碱解氮和有机质随土层加深表现...},
 author = {许, 俊丽 and 张, 桂莲 and 张, 希金 and 高, 志文 and 仲, 启铖 and 张, 亚萍 and 宋, 坤 and 达, 良俊},
 issn = {2095-0756},
 journal = {浙江农林大学学报},
 keywords = {age group, forest plantations, forest soil science, forest type, physical and chemical properties, soil, 人工林, 土壤, 森林土壤学, 植被类型, 理化性质, 龄组},
 language = {中文;},
 number = {06},
 pages = {1017--1026},
 title = {上海市人工林土壤理化性质与群落特征的相关性},
